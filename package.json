{
    "name": "vsc-redshift-extension",
    "displayName": "vsc_redshift_extension",
    "description": "A VSC extension to support custom cql language",
    "publisher": "0",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "cql",
            "aliases": ["cql", "cql"],
            "extensions": [".cql"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cql",
            "scopeName": "source.cql",
            "path": "./syntaxes/cql.tmLanguage.json"
        }]
    }
}
